<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title>Who's App</title>
    <link rel="icon" type="image/icon" href="/images/LogoIcon.png" />
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-Zenh87qX5JnK2Jl0vWa8Ck2rdkQ2Bzep5IDxbcnCeuOxjzrPF/et3URy9Bv1WTRi"
      crossorigin="anonymous"
    />
    <link rel="stylesheet" href="css/mainStyles.css">
  </head>
  <body class="theBody">
    <nav class="navbar navbar-dark bg-dark justify-content-center">
       {{#if contacts }}
        <div class="nav">
          <li class="nav-item">
            <a class="btn back" onclick="history.back()" >
            <svg class="w-6 h-6" fill="none" stroke="currentColor" width = "30px" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M3 12l2-2m0 0l7-7 7 7M5 10v10a1 1 0 001 1h3m10-11l2 2m-2-2v10a1 1 0 01-1 1h-3m-6 0a1 1 0 001-1v-4a1 1 0 011-1h2a1 1 0 011 1v4a1 1 0 001 1m-6 0h6"></path></svg>
            </a>
          </li>
        </div>
        {{/if}}

        {{#if socketOneChat }}
        <div class="nav">
          <li class="nav-item">
            <a class="btn back" onclick="history.back()" >
            <svg class="w-6 h-6" fill="none" stroke="currentColor" width = "20px" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M3 12l2-2m0 0l7-7 7 7M5 10v10a1 1 0 001 1h3m10-11l2 2m-2-2v10a1 1 0 01-1 1h-3m-6 0a1 1 0 001-1v-4a1 1 0 011-1h2a1 1 0 011 1v4a1 1 0 001 1m-6 0h6"></path></svg>
            </a>
          </li>
        </div>
        {{/if}}

     
        {{#if logged_in}}
        <div class="nav"> <button class="btn logout" id="logout">
            Logout <svg class="w-6 h-6" fill="none" stroke="currentColor" viewBox="0 0 24 24" width= "30px" xmlns="http://www.w3.org/2000/svg"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M11 16l-4-4m0 0l4-4m-4 4h14m-5 4v1a3 3 0 01-3 3H6a3 3 0 01-3-3V7a3 3 0 013-3h7a3 3 0 013 3v1"></path></svg>
          </button>
        </li></div>
        {{/if}}
        <div class="nav owl justify-content-center">
          <a class="navbar-brand mx-1" href="/login">
            <img
              src="/images/LogoIcon.png"
              alt="Logo"
              width="100"
              height="auto"
            />
          </a>
        </div>
        {{#if logged_in}}
        <div class="nav">
          <li class="nav-item">
            <a class="btn contacts" href="/contacts">
              Nest <svg class="w-6 h-6" fill="none" stroke="currentColor" viewBox="0 0 24 24" width="30px" xmlns="http://www.w3.org/2000/svg"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M17 20h5v-2a3 3 0 00-5.356-1.857M17 20H7m10 0v-2c0-.656-.126-1.283-.356-1.857M7 20H2v-2a3 3 0 015.356-1.857M7 20v-2c0-.656.126-1.283.356-1.857m0 0a5.002 5.002 0 019.288 0M15 7a3 3 0 11-6 0 3 3 0 016 0zm6 3a2 2 0 11-4 0 2 2 0 014 0zM7 10a2 2 0 11-4 0 2 2 0 014 0z"></path></svg>
            </a>
          </li>
        </div>
        {{/if}}
       
            {{#if contacts}}
        <div class="nav">
           <div class="nav"> <button class="btn logout" id="logout">
            <svg class="w-6 h-6" fill="none" stroke="currentColor" viewBox="0 0 24 24" width= "30px" xmlns="http://www.w3.org/2000/svg"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M11 16l-4-4m0 0l4-4m-4 4h14m-5 4v1a3 3 0 01-3 3H6a3 3 0 01-3-3V7a3 3 0 013-3h7a3 3 0 013 3v1"></path></svg>
          </button>
        </div>
        {{/if}}

          {{#if socketOneChat }}
        <div class="nav btn">
           <div class="row "><div
          class="col room "
          id="rooms"
        ></div></div>
        </div>
        {{/if}}

        
     
    
    </nav>
   

    <main class="mx-3">

      {{{body}}}
    </main>

    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-OERcA2EqjJCMA+/3y+gxIOqMEjwtxJY7qPCqsdltbNJuaOe923+mo//f6V8Qbsw3"
      crossorigin="anonymous"
    ></script>
    {{#if logged_in}}
      <script src="/js/logout.js" type="text/javascript"></script>
    {{/if}}

  </body>
</html>